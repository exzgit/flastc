class Calculator {
    pub let a: int = 100;
    pub let b: int = 200;

    pub func add() -> int {
        let result: int = self.a + self.b;
        return result;
    }
}

func main() -> int {
    let calc: Calculator = new Calculator();
    let result: int = calc.add();
    println("Addition: ", result);
    return EXIT_SUCCESS;
} 