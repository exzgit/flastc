func multiply(a: int, b: int) -> int {
    return a * b;
}

func add(a: int, b: int) -> int {
    return a + b;
}

func main() -> int {
    let x: int = 15;
    let y: int = 25;
    
    let sum: int = add(x, y);
    let product: int = multiply(x, y);
    
    println("Numbers: ", x);
    println("And: ", y);
    println("Sum: ", sum);
    println("Product: ", product);
    println("Demo completed successfully!");
    
    return EXIT_SUCCESS;
} 